{
    "env": {
        "browser": true,
        "jquery": true,
        "es2020": true,
        "commonjs": true
    },
    "extends": ["eslint:recommended", "google", "plugin:jest/recommended"],
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "plugins": ["jest"],
    "parser": "@babel/eslint-parser",
    "rules": {
        "complexity": [
            "error",
            10
        ],
        "indent": [
            "error",
            "tab"
        ],
        "no-tabs": 0,
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ]
    },
    "presets": [
        "@babel/preset-env"
    ]
}
