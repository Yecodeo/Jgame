{
    "parser": "@babel/eslint-parser",
    "presets": ["@babel/preset-env"],
    "env": {
        "browser": true,
        "jquery": true,
        "es2020": true,
        "commonjs": true
    },
    "extends": ["eslint:recommended", "google"],
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "indent": [
            2,
            "tab"
        ],
        "no-tabs": 0,
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ]
    }
}